# Docker .gitignore

.DS_Store
# Ignore Docker environment variable files
*.env

# Ignore Docker volumes and data directories
volumes/
data/

# Ignore Docker build output
build/

# Ignore Docker compose override files
docker-compose.override.yml

# Ignore Docker temporary files
.dockerignore
**/.dockerenv
**/.dockerinit

# Ignore default Docker files that should not be committed
# Dockerfile
# *.dockerfile
# docker-compose.yml

# Ignore generated image files
*.tar
*.tar.gz

# Ignore log files generated by containers
*.log
logs/

# Ignore security/secret files
secrets/
*.key
*.pem
*.crt

# Ignore specific Docker configuration files that may contain sensitive information
docker.env
.env.local
.env.development.local
.env.test.local
.env.production.local
.env

*.sql